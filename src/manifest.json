{
	"name": "H5Converter",
	"description": "Easily upload your artboards to h5mag.com",
	"version": "1.0",
	"author": "H5mag",
	"compatibleVersion": 3,
	"homepage": "h5mag.com",
	"authorEmail": "support@h5mag.com",
	"icon": "icon.png",
	"commands": [
		{
			"name": "Export",
			"identifier": "h5converter.my-command-identifier",
			"script": "./plugin.js",
			"shortcut": "ctrl shift .",
			"handlers": {
				"run": "onRun",
				"actions": {
					"Shutdown": "onShutdown"
				}
			}
		},
		{
			"name": "Fonts",
			"identifier": "h5converter.my-command-identifier-2",
			"script": "./plugin.js",
			"handlers": {
				"run": "onFonts",
				"actions": {
					"Shutdown": "onShutdown"
				}
			}
		},
		{
			"name": "Settings",
			"identifier": "h5converter.my-command-identifier-4",
			"script": "./plugin.js",
			"handlers": {
				"run": "onSettings",
				"actions": {
					"Shutdown": "onShutdown"
				}
			}
		},
		{
			"name": "Logout",
			"identifier": "h5converter.my-command-identifier-3",
			"script": "./plugin.js",
			"handlers": {
				"run": "onLogout",
				"actions": {
					"Shutdown": "onShutdown"
				}
			}
		}
	],
	"menu": {
		"title": "H5Converter",
		"items": [
			"h5converter.my-command-identifier",
			"h5converter.my-command-identifier-2",
			"h5converter.my-command-identifier-4",
			"h5converter.my-command-identifier-3"
		]
	}
}